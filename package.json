{
  "name": "tasks-pizza",
  "version": "1.0.0",
  "description": "A sample application using Cloud Tasks and Google Maps (to find pizza).",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "start": "FUNCTION_SOURCE=src npx @google-cloud/functions-framework",
    "deploy": "gcloud functions deploy tasks-pizza --trigger-http --runtime=nodejs10 --env-vars-file=.env.yaml"
  },
  "dependencies": {
    "@google-cloud/firestore": "^2.3.0",
    "@google-cloud/functions-framework": "^1.3.2",
    "@google-cloud/tasks": "^1.4.0",
    "@google/maps": "^0.5.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "firebase-admin": "^8.6.0",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/google__maps": "^0.5.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoogleCloudPlatform/tasks-pizza.git"
  },
  "author": "Grant Timmerman",
  "homepage": "https://github.com/GoogleCloudPlatform/tasks-pizza#readme"
}
